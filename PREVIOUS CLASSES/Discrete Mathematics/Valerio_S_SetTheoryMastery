    "#Work goes here\n",
    "\n",
    "q1=len(XONE)+len(PS4)\n",
    "q2=len(NES)+len(SegaM)+len(SNES)+len(SegaG)\n",
    "\n",
    "#------\n",
    "\n",
    "A=PS1^PS2\n",
    "B=PS3^PS4\n",
    "PS_Total=A^B\n",
    "\n",
    "C=XBX^X360\n",
    "BOX_Total=C^XONE\n",
    "\n",
    "Symmetric_Difference=PS_Total^BOX_Total\n",
    "q3=len(Symmetric_Difference)\n",
    "\n",
    "#------\n",
    "\n",
