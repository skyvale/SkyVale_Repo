    "X360 = {'Juan','Eoikka','Christian','Johnathan','Zac','Eric','Austin','Moises','Tyler','Theodore','Oswaldo','Cory','Jameson','Ronald','Dakota','Justin','Sofiane'}\n",
    "PS3 = {'Juan','Eric','Austin','Moises','Tyler','Theodore','Oswaldo','Curtis','Devin','Robert','Cory','Dakota','Justin','Chase','Sofiane'}\n",
    "XONE = {'Eoikka','Johnathan','Zac','Austin','Moises','Tony','Tyler','Theodore','Robert','Jameson','Alex','Dakota','Justin'}\n",
